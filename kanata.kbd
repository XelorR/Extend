;; Note on layout switching.
;;
;; My language binding for system layouts:
;; - Ctrl-8 = Qwerty (colemak and diktor_unicode layers depends on it)
;; - Ctrl-9 = –ô—Ü—É–∫–µ–Ω (diktor layer depends on it)
;;
;; In this configuration I heavily rely on C-8 and C-9 shortcuts,
;; which can be set up in KDE Plasma or Windows 11 via system settings.
;;
;; If C-8 snd C-9 are not binded, it is possible to use
;;   Colemak and Diktor unicode version with US layout only.

(defcfg concurrent-tap-hold yes)
(defchordsv2
  ;; Switching languages with chords
  ;; Colemak
      (w e) @cmk $hold-time all-released (gaming_split gaming navigation num mouse symbol)
  ;; Qwerty, no modtaps
      (x c) @gam $hold-time all-released (gaming_split gaming navigation num mouse symbol)
  ;; Gaming layout for split keyboard
      (z x) @gams $hold-time all-released (gaming_split gaming navigation num mouse symbol)
  ;; Diktor
      (i o) @dkt $hold-time all-released (navigation num mouse symbol)
  ;; Diktor unicode implementation
      (, .) @dktu $hold-time all-released (navigation num mouse symbol)
)

;; Variables and other settings
(defvar
  tap-time 200
  hold-time 200

  hold-index 150
  hold-middle 150
  hold-ring 170
  hold-pinky 195
)

(defalias
  ;; system combinations to switch languages
  en C-8
  ru C-9

  ;; navigation "Extend" layer
  spc (tap-hold $tap-time $hold-time spc (layer-toggle navigation))
  nav (layer-toggle navigation)

  ;; numeric, function and media layer
  num (tap-hold $tap-time $hold-time tab (layer-toggle num))
  fun (tap-hold $tap-time $hold-time ret (layer-toggle num))
  inv (tap-hold $tap-time $hold-time i (layer-toggle gaming_fn))
  g (tap-hold $tap-time $hold-time g (layer-toggle num))
  esc (tap-hold $tap-time $hold-time esc (layer-toggle num))
  ent (tap-hold $tap-time $hold-time ret (layer-toggle num))
  _' (tap-hold $tap-time $hold-time ' (layer-toggle num))
  _/ (tap-hold $tap-time $hold-pinky / (layer-toggle num))

  ;; symbol layer
  ralt (tap-hold $tap-time $hold-time ralt (layer-toggle symbol))
  mnu (tap-hold $tap-time $hold-time ralt (layer-toggle symbol))

  ;; mouse layer
  z (tap-hold $tap-time $hold-pinky z (layer-toggle mouse))

  ;; layout layers
  cmk (multi (layer-switch colemak) @en)
  dkt (multi (layer-switch diktor) @ru)
  dktu (multi (layer-switch diktor_unicode) @en)
  gam (multi (layer-switch gaming) @en)
  gams (multi (layer-switch gaming_split) @en)

  ;; launchers
  term (macro lmet 300 S-t e r m i n a l 400 ret)
  terr (macro @en 50 lmet 300 S-t e r m i n a l 400 ret 50 @ru)
  vscode (macro lmet 300 S-v i s u a l 50 spc S-s t u d i o 50 spc S-c o d e 400 ret)
  vscodr (macro @en 50 lmet 300 S-v i s u a l 50 spc S-s t u d i o 50 spc S-c o d e 400 ret 50 @ru)
  vsc (switch
    ((base-layer diktor)) @vscodr break
    () @vscode break
  )
  ter (switch
    ((base-layer diktor)) @terr break
    () @term break
  )

  ;; unicode
  u‚Äî (unicode ‚Äî)
  ur‚Äî (macro @en 50 @u‚Äî 50 @ru)
  ‚Äî (switch
    ((base-layer diktor)) @ur‚Äî break
    () @u‚Äî break
  )
  u¬´ (unicode ¬´)
  u¬ª (unicode ¬ª)
  u¬´¬ª (macro @u¬´ 50 @u¬ª 50 left)
  ur¬´¬ª (macro @en 50 @u¬´ 50 @u¬ª 50 left 50 @ru)
  u‚Äú (unicode ‚Äú)
  u‚Äù (unicode ‚Äù)
  u‚Äú‚Äù (macro @u‚Äú 50 @u‚Äù 50 left)
  ur‚Äú‚Äù (macro @en 50 @u‚Äú 50 @u‚Äù 50 left 50 @ru)
  ¬´‚Äù (switch
    ((base-layer diktor_unicode)) @u¬´¬ª break
    ((base-layer diktor)) @ur¬´¬ª break
    () @u‚Äú‚Äù break
  )

  ;; shortcuts
  cw (caps-word 2000)
  fav C-S-o
  undo C-z
  cut C-x
  copy C-c
  paste C-v
  redo C-S-v
  ‚éà (one-shot 500 lctl)

  ;; symbols for num layer
  dq (switch
    ((base-layer diktor)) S-2 break
    () S-' break
  )
  < (switch
    ((base-layer diktor)) (macro @en 50 S-, 50 @ru) break
    () S-, break
  )
  > (switch
    ((base-layer diktor)) (macro @en 50 S-. 50 @ru) break
    () S-. break
  )
  ` (switch
    ((base-layer diktor)) (macro @en 50 grv 50 @ru) break
    () grv break
  )
  ~ (switch
    ((base-layer diktor)) (macro @en 50 S-grv 50 @ru) break
    () S-grv break
  )
  / (switch
    ((base-layer diktor)) S-\ break
    () / break
  )
  # (switch
    ((base-layer diktor)) (macro @en 50 S-3 50 @ru) break
    () S-3 break
  )
  ^ (switch
    ((base-layer diktor)) (macro @en 50 S-6 50 @ru) break
    () S-6 break
  )
  \ (tap-hold $tap-time $hold-pinky \ lmet)
  lb (tap-hold $tap-time $hold-ring S-9 lalt)
  - (tap-hold $tap-time $hold-middle - lctl)
  rb (tap-hold $tap-time $hold-index S-0 lsft)
  [ (switch
    ((base-layer diktor)) (macro @en 50 [ 50 @ru) break
    () [ break
  )
  ] (switch
    ((base-layer diktor)) (macro @en 50 ] 50 @ru) break
    () ] break
  )
  { (tap-hold $tap-time $hold-index (switch
    ((base-layer diktor)) (macro @en 50 S-[ 50 @ru) break
    () S-[ break
  ) lsft)
  } (tap-hold $tap-time $hold-ring (switch
    ((base-layer diktor)) (macro @en 50 S-] 50 @ru) break
    () S-] break
  ) lalt)
  | (switch
    ((base-layer diktor)) (macro @en 50 S-\ 50 @ru) break
    () S-\ break
  )
  $ (switch
    ((base-layer diktor)) (macro @en 50 S-4 50 @ru) break
    () S-4 break
  )
  & (switch
    ((base-layer diktor)) (macro @en 50 S-7 50 @ru) break
    () S-7 break
  )
  ' (switch
    ((base-layer diktor)) (macro @en 50 ' 50 @ru) break
    () ' break
  )
  at (switch
    ((base-layer diktor)) (macro @en 50 S-2 50 @ru) break
    () S-2 break
  )
  : (switch
    ((base-layer diktor)) S-6 break
    () S-; break
  )
  ; (tap-hold $tap-time $hold-middle (switch
    ((base-layer diktor)) S-4 break
    () ; break
  ) lctl)
  , (switch
    ((base-layer diktor)) S-/ break
    () , break
  )
  . (switch
    ((base-layer diktor)) / break
    () . break
  )
  ! (tap-hold $tap-time $hold-pinky S-1 lmet)
  ? (switch
    ((base-layer diktor)) S-7 break
    () S-/ break
  )
  4h (tap-hold $tap-time $hold-index 4 lsft)
  5h (tap-hold $tap-time $hold-middle 5 lctl)
  6h (tap-hold $tap-time $hold-ring 6 lalt)
  f2 (tap-hold $tap-time $hold-middle f2 lalt)
  f4 (tap-hold $tap-time $hold-ring f4 lalt)
  f5 (tap-hold $tap-time $hold-middle f5 lctl)
  f6 (tap-hold $tap-time $hold-index f6 lsft)
  f11 (tap-hold $tap-time $hold-pinky f11 lmet)
  bspc (tap-hold $tap-time $hold-pinky bspc lmet)
  * S-8

  ;; mouse
  üñ±‚Üë (movemouse-up 1 1)
  üñ±‚Üì (movemouse-down 1 1)
  üñ±‚Üê (movemouse-left 1 1)
  üñ±‚Üí (movemouse-right 1 1)
